### Terminal Environment

set fish_greeting "Hello."

# Paste board
set -x FISH_CLIPBOARD_CMD pbcopy

# Colors
set -x TERM screen-256color

# Like to use vim?
set -x EDITOR vim

# Where is the ssl cert bundle?
set -x SSL_CERT_FILE /usr/local/opt/curl-ca-bundle/share/ca-bundle.crt

set -x BUNDLE_JOBS 4

