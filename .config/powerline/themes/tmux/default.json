{
    "default_module": "powerline.segments.common",
    "segment_data": {
        "external_ip": {
            "before": ""
        },
        "date": {
          "before": "  "
        }
    },
    "segments": {
        "right": [
            {
                "name": "external_ip"
            },
            {
                "name": "weather",
                "args": {
                  "interval": 1800
                }
            },
            {
                "name": "date",
                "args": {
                    "format": "%H:%M",
                    "istime": true
                }
            }
        ]
    }
}
