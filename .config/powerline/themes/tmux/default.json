{
    "default_module": "powerline.segments.common",
    "segment_data": {
        "uptime": {
            "before": "⇑  "
        },
        "external_ip": {
            "before": "ⓦ  "
        },
        "date": {
          "before": "  "
        }
    },
    "segments": {
        "right": [
            {
                "name": "uptime"
            },
            {
                "name": "external_ip"
            },
            {
                "name": "network_load"
            },
            {
                "name": "system_load"
            },
            {
                "name": "weather",
                "args": {
                  "location_query": "Auckland, New Zealand"
                }
            },
            {
                "name": "date",
                "args": {
                    "format": "%H:%M",
                    "istime": true
                }
            }
        ]
    }
}
