{
    "default_module": "powerline.segments.common",
    "segment_data": {
        "uptime": {
            "before": "⇑  "
        },
        "external_ip": {
            "before": "ⓦ  "
        },
        "date": {
            "before": "⌚ "
        },
        "weather": {
          "args": {
            "units": "c",
            "location_query": "Auckland, New Zealand"
          }
        }
    },
    "segments": {
        "right": [
            {
                "name": "uptime",
                "priority": 50
            },
            {
                "name": "external_ip",
                "priority": 50
            },
            {
                "name": "network_load",
                "priority": 50
            },
            {
                "name": "system_load",
                "priority": 50
            },
            {
                "name": "weather",
                "priority": 50
            },
            {
                "name": "date",
                "args": {
                    "format": "%H:%M",
                    "istime": true
                }
            },
            {
                "name": "hostname"
            }
        ]
    }
}
